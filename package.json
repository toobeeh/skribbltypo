{
  "name": "skribbltypo",
  "displayName": "skribbltypo",
  "version": "28.0.2",
  "author": "tobeh",
  "description": "The toolbox for everything you need on skribbl.io",
  "type": "module",
  "engines": {
    "node": ">=14.18.0"
  },
  "scripts": {
    "dev": "vite build --watch --mode development",
    "dev-hmr": "vite",
    "build-dev": "npm run generate:workers && npm run lint && vite build --mode development",
    "build-prod": "npm run generate:workers && npm run lint && vite build --mode production",
    "build-prod-userscript": "npm run generate:workers && npm run lint && cross-env TARGET=userscript vite build --mode production",
    "build-prod-chrome": "npm run generate:workers && npm run lint && cross-env TARGET=chrome vite build --mode production",
    "build-prod-firefox": "npm run generate:workers && npm run lint && cross-env TARGET=firefox vite build --mode production",
    "lint": "eslint . ",
    "lint-fix": "eslint . --fix",
    "test": "vitest --config vitest.config.ts",
    "generate:patch": "npx tsx codegen/executePatch.ts",
    "generate:workers": "npx tsx codegen/generateWorkers.ts",
    "generate:api": "openapi-generator-cli generate -i https://api.typo.rip/openapi.json -g typescript-fetch -o src/api/ -p=removeOperationIdPrefix=true",
    "generate:signalr": "dotnet tsrts --project C:/Users/tobeh/repos/Avallone/tobeh.Avallone.Server/tobeh.Avallone.Server.csproj --output src/signalr"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.0.0-beta.30",
    "@eslint/js": "^9.11.0",
    "@openapitools/openapi-generator-cli": "^2.13.5",
    "@sveltejs/vite-plugin-svelte": "3.1.2",
    "@types/chrome": "^0.0.246",
    "@types/js-beautify": "^1.14.3",
    "@types/node": "^22.13.13",
    "@types/omggif": "^1.0.5",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.25.0",
    "eslint": "^9.11.0",
    "globals": "^15.9.0",
    "gulp": "^4.0.2",
    "gulp-zip": "^6.0.0",
    "js-beautify": "^1.15.1",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "sass": "^1.79.3",
    "svelte": "^4.2.19",
    "svelte-preprocess": "^6.0.2",
    "tslib": "^2.6.2",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.8",
    "vite-plugin-checker": "^0.8.0",
    "vite-plugin-monkey": "^5.0.8",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.5"
  },
  "dependencies": {
    "@abraham/reflection": "^0.12.0",
    "@microsoft/signalr": "^8.0.7",
    "idb": "^8.0.2",
    "inversify": "^6.0.2",
    "omggif": "^1.0.10",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "svelte-markdown": "^0.4.1"
  }
}
